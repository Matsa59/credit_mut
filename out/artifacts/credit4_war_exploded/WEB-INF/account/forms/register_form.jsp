<s:form action="sign-up" method="POST">
    <input type="text" name="userEntity.username" placeholder="Username" class="field"><br />
    <input type="password" name="userEntity.password" placeholder="Password" class="field"><br />
    <input type="text" name="userEntity.lastName" placeholder="Lastname" class="field"><br />
    <input type="text" name="userEntity.firstName" placeholder="Firstname" class="field"><br />
    <input type="text" name="userEntity.email" placeholder="Email" class="field"><br />
    <input type="submit" value="Sign up" class="field submit">
</s:form>